.container-dialog-mask {
    background: #000;
    color: #fff;
    height: 100%;
    left: 0;
    opacity: .5;
    overflow: hidden;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 9000;
}

.container-dialog-content {
    background: #1a1d26;
    border-radius: 36rpx;
    color: #fff;
    left: 0;
    margin: -200rpx 54rpx;
    overflow: hidden;
    position: fixed;
    top: 50%;
    width: 86%;
    z-index: 9999;
}

.txt-title {
    color: #fff;
    font-size: 36rpx;
    font-weight: 500;
    padding-top: 50rpx;
    text-align: center;
}

.container-center-input {
    border-radius: 4rpx;
    display: flex;
    font-size: 28rpx;
    margin-left: 20rpx;
    padding: 50rpx 32rpx;
    width: 86%;
}

.input-dump-name {
    border-bottom: 1px solid #262931;
    box-sizing: border-box;
    color: #f5f5f5;
    font-size: 34rpx;
    font-weight: 500;
    height: 96rpx;
    letter-spacing: 2rpx;
    line-height: 28rpx;
    padding: 0 20rpx;
    width: 100%;
}

.input-holder {
    color: #fff;
    font-size: 28rpx;
}

.phClass {
    font-size: 34rpx;
}

.container-bottom-btns {
    border-top: 1px solid #262931;
    display: flex;
    flex-direction: row;
    font-size: 34rpx;
    height: 86rpx;
    line-height: 86rpx;
}

.btn-cancel {
    border-right: 1px solid #262931;
    color: #fff;
}

.btn-cancel,.btn-confirm {
    flex: 1;
    text-align: center;
    width: 100%;
}

.btn-confirm {
    color: #f5a200;
}