.container-icon {
    box-sizing: border-box;
    color: #fff;
    font-size: 36rpx;
    font-weight: 500;
    padding-bottom: 24rpx;
    padding-top: 58rpx;
    text-align: center;
}

.container-title {
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    padding-bottom: 46rpx;
    padding-left: 30rpx;
    padding-right: 30rpx;
    width: 100%;
}

.txt-title-main {
    color: #fff;
    font-size: 34rpx;
    font-weight: 700;
    line-height: 40rpx;
}

.txt-title-main,.txt-title-sub {
    letter-spacing: 2rpx;
    text-align: center;
    width: 100%;
}

.txt-title-sub {
    box-sizing: border-box;
    font-size: 26rpx;
    font-weight: 400;
    line-height: 26rpx;
    line-height: 40rpx;
    margin-top: 12rpx;
    padding-left: 30rpx;
    padding-right: 30rpx;
}

.txt-title-sub-fail {
    color: #838b99;
}

.txt-title-sub-succ {
    color: #999;
}

.btn-bottom-hover {
    background: #0f1012;
}

.container-button {
    align-items: center;
    border-top: 2rpx solid #262931;
    display: flex;
    flex-direction: row;
    font-size: 34rpx;
    justify-content: center;
    line-height: 86rpx;
    width: 100%;
}

.btn-base {
    box-sizing: border-box;
    flex: 1;
    font-size: 34rpx;
    font-weight: 500;
    height: 100%;
    letter-spacing: 2rpx;
    line-height: 48rpx;
    padding-bottom: 32rpx;
    padding-top: 32rpx;
    text-align: center;
}

.btn-cancel {
    color: #fff;
}

.btn-confirm {
    border-left: 2rpx solid hsla(0,0%,100%,.05);
}

.btn-complete,.btn-confirm {
    color: #7d90a9;
}

.icon-success {
    background: url("data:image/svg+xml;base64,");
    background-size: 80rpx 80rpx;
    height: 80rpx;
    width: 80rpx;
}

.icon-fail {
    background: url("data:image/svg+xml;base64,");
    background-size: 80rpx 80rpx;
    height: 80rpx;
    width: 80rpx;
}