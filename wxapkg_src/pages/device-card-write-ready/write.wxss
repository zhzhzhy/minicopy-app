.animation {
    bottom: -300rpx;
    height: 300rpx;
    margin-left: -50rpx;
    position: relative;
    transform: scale(.5);
    width: 423.5714rpx;
}

.box2 {
    margin-top: -30rpx;
    position: relative;
    top: 40rpx;
}

.box2,.box3 {
    animation: myfirst 3s 2s infinite both;
    background: linear-gradient(to bottom right,#fff,#fff 60%,hsla(0,0%,87%,0) 0,rgba(245,162,0,.49));
    height: 400rpx;
    margin-left: 10rpx;
    transform: rotate(-135deg);
    width: 400rpx;
}

.box3 {
    margin-top: -280rpx;
}

@-webkit-keyframes myfirst {
    0% {
        opacity: 1;
    }

    30% {
        opacity: .8;
    }

    50% {
        opacity: .5;
    }

    60% {
        opacity: .2;
    }

    70% {
        opacity: .5;
    }

    80% {
        opacity: .8;
    }

    100% {
        opacity: 1;
    }
}

@keyframes myfirst {
    0% {
        opacity: 1;
    }

    30% {
        opacity: .8;
    }

    50% {
        opacity: .5;
    }

    60% {
        opacity: .2;
    }

    70% {
        opacity: .5;
    }

    80% {
        opacity: .8;
    }

    100% {
        opacity: 1;
    }
}

